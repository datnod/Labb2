<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>


<GridPane alignment="center" hgap="10" vgap="10" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/9.0.1" fx:controller="lab2.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <AnchorPane prefHeight="200.0" prefWidth="200.0">
         <children>
            <Button fx:id="getButton" layoutX="14.0" layoutY="30.0" mnemonicParsing="false" onAction="#getMove" text="Get latest" />
            <Button fx:id="sendButton" layoutX="14.0" layoutY="88.0" mnemonicParsing="false" onAction="#sendMove" prefHeight="25.0" prefWidth="67.0" text="Send" />
            <TextField fx:id="sendField" layoutX="100.0" layoutY="88.0" prefHeight="25.0" prefWidth="67.0" />
            <TextField fx:id="getField" editable="false" layoutX="100.0" layoutY="30.0" prefHeight="25.0" prefWidth="67.0" />
         </children>
      </AnchorPane>
   </children>
</GridPane>
